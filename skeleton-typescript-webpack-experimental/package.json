{
  "name": "skeleton-typescript-webpack-experimental",
  "version": "1.0.0",
  "description": "A starter kit for building a standard navigation-style app with Aurelia and Webpack.",
  "main": "dist/app.bundle.js",
  "repository": {
    "url": "git+ssh://git@github.com/aurelia/skeleton-navigation.git",
    "type": "git"
  },
  "author": "Bazyli Brz√≥ska <bazyli.brzoska@gmail.com> (https://invent.life/)",
  "license": "CC0-1.0",
  "scripts": {
    "test": "nps test",
    "start": "node dist/server.bundle.js",
    "watch": "nps webpack.server.ssr.watch",
    "server": "nps webpack.server.ssr.start"
  },
  "engines": {
    "node": "8.1.2"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.spec\\.(ts|js)x?$",
    "setupFiles": [
      "<rootDir>/test/jest-pretest.ts"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "aurelia-(.*)": "<rootDir>/node_modules/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!**/*.spec.{js,ts}",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "<rootDir>/test/coverage-jest",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "mapCoverage": true
  },
  "devDependencies": {
    "@types/jest": "19.2.4",
    "@types/jsdom": "^2.0.29",
    "@types/lodash": "4.14.65",
    "@types/node": "7.0.29",
    "@types/webpack": "2.2.15",
    "aurelia-protractor-plugin": "^1.0.1",
    "aurelia-testing": "^1.0.0-beta.2.0.1",
    "aurelia-webpack-plugin": "^2.0.0-rc.2",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "^0.11.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "html-webpack-plugin": "^2.26.0",
    "http-server": "^0.9.0",
    "jest": "^18.1.0",
    "jest-cli": "20.0.4",
    "nps": "5.3.1",
    "nps-utils": "1.2.0",
    "postcss-loader": "2.0.6",
    "protractor": "^5.0.0",
    "require-dir": "^0.3.2",
    "style-loader": "0.18.2",
    "ts-jest": "^18.0.1",
    "ts-node": "^3.0.6",
    "typescript": "2.3.4",
    "url-loader": "^0.5.7",
    "wait-on": "^2.0.1",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "aurelia-bootstrapper": "2.1.1",
    "aurelia-fetch-client": "1.1.2",
    "aurelia-framework": "1.1.2",
    "aurelia-history-browser": "1.0.0",
    "aurelia-loader-webpack": "2.1.0",
    "aurelia-logging-console": "1.0.0",
    "aurelia-pal-browser": "1.2.1",
    "aurelia-polyfills": "1.2.1",
    "aurelia-router": "1.3.0",
    "aurelia-history": "^1.0.0",
    "aurelia-templating": "1.4.2",
    "aurelia-templating-binding": "1.3.0",
    "aurelia-templating-resources": "1.4.0",
    "aurelia-templating-router": "1.1.0",
    "autoprefixer": "^7.1.1",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "koa": "^2.3.0",
    "koa-static": "^3.0.0",
    "aurelia-pal-nodejs": "1.0.0-beta.1.0.0",
    "ejs": "^2.5.6",
    "preboot": "^4.5.2"
  }
}
